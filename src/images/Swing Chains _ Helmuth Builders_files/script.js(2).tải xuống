    if(event.detail.contactFormId == 4648){
      if(!$('#term:checkbox:checked').length > 0){
        $(".custom-checkbox").addClass('error');
        $(".custom-checkbox").prepend('<span role="alert" class="wpcf7-not-valid-tip">The field is required.</span>');
      }
    }      
  }, false );

