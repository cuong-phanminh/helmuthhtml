    //     var img_data = canvas.toDataURL('image/jpeg');
    //     $('.wpcf7-form input.image-shedbuilder').val(img_data);
    //     // if(canvas){
    //        $.ajax({
    //             url: wc_add_to_cart_params.ajax_url,
    //             method: 'post',
    //             data: { 
    //                 'action': 'save_image_to_server',
    //                 'image_data': img_data,
